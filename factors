#!/usr/bin/python3
import sys
if len(sys.argv) != 2:
    print("Usage: ./factors <file>")
    exit(1)
arg = sys.argv[1]
def do_prime(numbers):
    for number in numbers:
        number = int(number)
        if number % 2 == 0:
            print(f"{number}={number // 2}*2")
        else:
            for n in range (3, int(number ** 0.5), 2):
                if number % n == 0:
                    print(f"{number}={number // n}*{n}")
                    break
try:
    fille = open(arg, "r")
    content = fille.readlines()
    do_prime(content)
    fille.close()
except:
    print("File not found")
    exit(1)
