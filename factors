#!/usr/bin/python3
import sys
import time


def do_prime(numbers):
    for number in numbers:
        number = int(number)
        if number % 2 == 0:
            print(f"{number}={number // 2}*2")
        elif number % 3 == 0:
            print(f"{number}={number // 3}*3")
        elif number % 5 == 0:
            print(f"{number}={number // 5}*5")
        elif number % 7 == 0:
            print(f"{number}={number // 7}*7")
        else:
            for n in range(3, int(number ** 0.5) + 1, 2):
                if number % n == 0:
                    print(f"{number}={number // n}*{n}")
                    break


def sortt(cont):
    with open(cont, "r") as fie:
        content = fie.readlines()
    sorted_lines = sorted(map(int, (line.strip() for line in content)))
    with open(cont, "w") as fie:
        fie.writelines(str(line) + "\n" for line in sorted_lines)
    fie.close()
    return (sorted_lines)


if __name__ == "__main__":
    try:
        if len(sys.argv) != 2:
            print("Usage: factors <file>")
            exit()
        arg = sys.argv[1]
        fille = sortt(arg)
        do_prime(fille)
    except FileNotFoundError:
        print("File not found")
        exit()
